services:
  pi.plataformaweb.enchente:
    build:
      context: E:\PI.PlataformaWeb\Pi.PlataformaWeb.Enchente
      dockerfile: Pi.PlataformaWeb.Enchente/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Pi.PlataformaWeb.Enchente
      target: base
    container_name: Pi.PlataformaWeb.Enchente
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: piplataformawebenchente:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/netcoreapp3.1/Pi.PlataformaWeb.Enchente.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - E:\PI.PlataformaWeb\Pi.PlataformaWeb.Enchente\Pi.PlataformaWeb.Enchente:/app:rw
    - C:\Users\Leonardo\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\Leonardo\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\Leonardo\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - C:\Users\Leonardo\.nuget\packages:/root/.nuget/packages:ro
    - E:\PI.PlataformaWeb\Pi.PlataformaWeb.Enchente:/src:rw
version: '3.4'